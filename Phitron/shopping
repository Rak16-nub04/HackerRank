#include<stdio.h>
int main(){
    int n;
    scanf("%d",&n);

    if(n<=1000)
        printf("Bad luck!");

        else if(n>1000 && (n-1000)>=500)
        printf("I will buy Punjabi\nI will buy new shoes\nAlisa will buy new shoes");

       else if(n>1000)
        printf("I will buy Punjabi");
        return 0;
}
